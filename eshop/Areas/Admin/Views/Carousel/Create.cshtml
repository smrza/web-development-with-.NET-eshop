@model Carousel
@{
    ViewData["Title"] = "Carousel Create";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<p>Create new carousel item.</p><br />

@{ViewBag.Action = "Create"; }

<partial name="EditForm" model="Model">

    @*@await Html.PartialAsync("EditForm", Model)*@

    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        <environment include="Development">
            <script src="~/js/Validation/FileContentTypeValidation.js"></script>
        </environment>
        <environment exclude="Development">
            <script src="~/js/Validation/FileContentTypeValidation.min.js"></script>
        </environment>
    }
